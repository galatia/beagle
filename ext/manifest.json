{
  "manifest_version": 2,
  "name": "Beagle",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "http://*/*.pdf"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "externally_connectable":
  {
    "matches": [
      "*://localhost/*"
    ]
  } ,
  "web_accessible_resources": [
    "pdfjs/*"
  ]
}
