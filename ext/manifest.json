{
  "manifest_version": 2,
  "name": "Beagle",
  "version": "0.0.2",
  "icons": {
    "16":  "logo/logo16.png",
    "32":  "logo/logo32.png",
    "48":  "logo/logo48.png",
    "128": "logo/logo128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*.pdf"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "externally_connectable":
  {
    "matches": [
      "*://localhost/*",
      "*://beagle.link/*"
    ]
  } ,
  "web_accessible_resources": [
    "pdfjs/*"
  ],
  "key": ""
}
